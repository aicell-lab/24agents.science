   "source": [
    "merged = target_hits.merge(\n",
    "    repurposing_smiles,\n",
    "    left_on='Ligand Name',\n",
    "    right_on='compound_name',\n",
    "    how='left'\n",
    ").merge(\n",
    "    repurposing_meta,\n",
    "    on='compound_id',\n",
    "    how='left'\n",
    ")"
   ]
  },
  {
   "cell_type": "code",
