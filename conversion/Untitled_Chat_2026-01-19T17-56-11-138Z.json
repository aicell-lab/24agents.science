    "TARGET_NAME = 'EGFR'\n",
    "\n",
    "target_hits = bindingdb[bindingdb['Target Name'].str.contains(TARGET_NAME, na=False)]\n",
    "target_hits = target_hits.dropna(subset=['Kd (nM)'])"
   ]
  },
  {
   "cell_type": "code",
